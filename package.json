    "grunt-contrib-qunit": "^1.2.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "19.0.0",
    "grunt-jsonlint": "1.1.0",
    "grunt-stylelint": "^0.6.0",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "stylelint-config-wikimedia": "0.3.0",
    "webpack": "^1.14.0"
  }
